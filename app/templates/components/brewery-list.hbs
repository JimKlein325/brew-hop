{{#if (await jsonResponse)}}
  {{google-map pos=pos venues=(await jsonResponse)}}
{{/if}}

<ul>
  {{#each (await jsonResponse) as |venue|}}
    <br>{{venue.name}}
    <br>{{venue.location.address}}
    <br>{{venue.location.lat}}, {{venue.location.lng}}
    <br><a href="{{venue.url}}">{{venue.url}}</a>
    <hr>
  {{/each}}
</ul>
